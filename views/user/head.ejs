    <style>
      .custom-alert{
          position:absolute; 
          top:20%; 
          left:0;            
          display: flex; 
          justify-content: space-between;
          align-items: center;
          padding: 20px;
          z-index:98; 
          width:100%; 
          height:100px;
          background-color: lightgreen;
          border: 3px solid darkgreen;
          border-radius: 5px;
          opacity: 0.7;
      }
      .custom-alert h2 {
          z-index: 99;
          font-size: 1rem;
          color: darkgreen;
      }
      .custom-alert .custom-close {            
          font-size: 1.5rem;
          font-weight: 900;
          color: darkgreen;
          background-color: transparent;
          border: none;          
          cursor: pointer;
          z-index: 99;
      }
